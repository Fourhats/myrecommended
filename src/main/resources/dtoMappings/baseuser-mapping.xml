<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping wildcard="false">
		<class-a>com.myrecommended.models.User</class-a>
		<class-b>com.myrecommended.services.users.dtos.BaseUserDTO
		</class-b>

		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>surname</a>
			<b>surname</b>
		</field>
		<field>
			<a>avatarName</a>
			<b>avatarName</b>
		</field>
	</mapping>
</mappings>